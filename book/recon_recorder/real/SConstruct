from rsf.proj import *

#
# To execute using 20 processors: scons -j 20 CLUSTER="localhost 48" 
# (or pscons if you want to use all 48 processors on neurus)

#
# Convert to RSF
#
Flow('din tdin','/u1/data/Petrobank/Whitesands_2009_PS/su/v.su',
     '''
     segyread su=y suxdr=y tfile=${TARGETS[1]} 
     ''') 

#
# Make headers needed for interpolation
# 
Flow('head_geom','tdin',
     '''
     geom5d headin=${SOURCES[0]} headout=${TARGETS[0]} 
            mode=2
            omx=480000 omy=6160000  
            dmx=10 dmy=5 dhx=100 dhy=50
            ang=127.752 
     ''') 

#
# Bin the data (zero traces at missing trace locations)
#
Flow('din_pad head_pad','din head_geom',
     '''
     pad5d headin=${SOURCES[1]} headout=${TARGETS[1]} mode=2
     ''') 

#
# apply agc
#
Flow('din_prep','din_pad',
     '''
     agc
     ''') 

#
# Patch midpoints in the x and y directions
#
Flow('din_patch','din_prep',
     '''
     patch5d w=1000,10,10,9,57 p=1,20,35,1,1 verb=y 
     ''')

#
# Process 700 patches of data
#
for imx in range(20):
    for imy in range(35):
        Flow('dorig_patch_%d_%d' % (imx,imy),'din_patch',
             '''
             window f7=%d n7=1 f8=%d n8=1         
             ''' % (imx,imy))
        Flow('dpocs_patch_%d_%d' % (imx,imy),'din_patch',
             '''
             window f7=%d n7=1 f8=%d n8=1 |
             pocs5d method=1 tw_length=200 iter=30 fmax=120 verbose=0        
             ''' % (imx,imy))
        Flow('dmwni_patch_%d_%d' % (imx,imy),'din_patch',
             '''
             window f7=%d n7=1 f8=%d n8=1 |
             pocs5d method=2 tw_length=200 iter=15 iter_i=3 fmax=120 verbose=0         
             ''' % (imx,imy))
        Flow('dseqsvd_patch_%d_%d' % (imx,imy),'din_patch',
             '''
             window f7=%d n7=1 f8=%d n8=1 |
             seqsvd5d method=3 tw_length=200 iter=30 ranki=8 rankf=8 fmax=120 verbose=0         
             ''' % (imx,imy))

#
# merge the patches
#
