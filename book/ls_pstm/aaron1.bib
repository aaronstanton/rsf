	Journal = {Geophysics},
	Number = {3},
	Pages = {383-392},
	Read = {1},
	Title = {Four-component surface-consistent deconvolution},
	Url = {http://geophysics.geoscienceworld.org/cgi/content/abstract/58/3/383},
