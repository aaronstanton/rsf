	Pages = {723--731},
	Publisher = {Blackwell Science Ltd},
	Title = {Instantaneous polarization attributes in the time--frequency domain and wavefield separation},
	Url = {http://dx.doi.org/10.1111/j.1365-2478.2005.00500.x},
